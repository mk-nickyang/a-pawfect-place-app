{
  "expo": {
    "name": "A Pawfect Place",
    "slug": "a-pawfect-place",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "apawfectplace",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "come.apawfectplace.app",
      "associatedDomains": [
        "applinks:apawfectplace.com.au"
      ],
      "buildNumber": "6",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "come.apawfectplace.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.apawfectplace.com.au"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "versionCode": 2
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "a-pawfect-place",
          "project": "react-native"
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "fa605c80-39cf-4295-836c-f59cda94fb12"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fa605c80-39cf-4295-836c-f59cda94fb12"
    }
  }
}
